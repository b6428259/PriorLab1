@startuml
title getAllInventories()

actor User as u
boundary API as a
database Database as d

u->a : GET /api/v1/inventory
a->d: findAll()
a->a : Validate inventories

alt Inventory found
    d-->a : inventories
    a->u : 200 OK
    a->u : "Data retrieved successfully"
    a->u : JSON Inventory data
else Inventory not found
    a->u : 404 Not Found
    a->u : "Inventory not found"
end

@enduml
